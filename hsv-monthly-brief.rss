---

---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom" xml:lang="{{ site.lang }}">
	<channel>
		<atom:link href="{{ site.url }}/hsv-monthly-brief.rss" rel="self" type="application/rss+xml" />
		<title>{{ site.title }}</title>
		<description>{{ site.description | xml_escape }}</description>
		<link>{{ site.url }}</link>
		<language>en-us</language>
		<managingEditor>{{ site.email }} ({{ site.author }})</managingEditor>
		<webMaster>{{ site.email }} ({{ site.author }})</webMaster>
		<copyright>2019-{{ site.time | "%Y" }} {{ site.title }}</copyright>
		<pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
		<lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
		<image>
			<link>{{ site.url }}</link>
			<url>{{ site.url }}/assets/images/tng_logo.png</url>
			<title>{{ site.title }}</title>
		</image>
		{% for post in site.publications.hsv-monthly %}
			<item>
				<title>{{ post.title | xml_escape }}</title>
				<link>{{ post.url }}</link>
				<pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
				<description>{{ post.headlines | xml_escape }}</description>
				<guid isPermaLink="true">{{ site.url }}{{ post.url }}</guid>
			</item>
		{% endfor %}
	</channel>
</rss>
